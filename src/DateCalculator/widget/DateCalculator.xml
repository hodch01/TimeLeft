<?xml version="1.0" encoding="utf-8" ?>
<widget id="DateCalculator.widget.DateCalculator" needsEntityContext="true" xmlns="http://www.mendix.com/widget/1.0/">
  <name>DateCalculator</name>
  <description></description>
   <icon>
</icon>
  <properties>
    <property key="dateAttr" type="attribute" defaultValue="" required="true">
      <caption>Date Value</caption>
      <category>Data source</category>
      <description>Date value to manipulate</description> 
      <attributeTypes>
        <attributeType name="DateTime"/> 
      </attributeTypes>
    </property>
    <property key="maxDate" type="integer" required="true" defaultValue="90">
      <caption>Max Date</caption>
      <category>Data source</category>
      <description>The max date value</description> 
    </property>
    <property key="expiredMsg" type="string" required="true" defaultValue="-">
        <caption>Expired Message</caption>
      <category>Data source</category>
      <description>The message/value for expired dates</description> 
    </property>
    <property key="expiredMetric" type="string" required="true" defaultValue="days">
        <caption>Expired Metric</caption>
      <category>Data source</category>
      <description>The message/value for the expired dates metric</description> 
    </property>
    <property key="invalidMsg" type="string" required="true" defaultValue="-">
        <caption>Invalid Message</caption>
      <category>Data source</category>
      <description>The message/value for invalid dates</description> 
    </property>
    <property key="invalidMetric" type="string" required="true" defaultValue="days">
        <caption>Invalid Metric</caption>
      <category>Data source</category>
      <description>The message/value for the invalid dates metric</description> 
    </property>  
  </properties>
</widget>